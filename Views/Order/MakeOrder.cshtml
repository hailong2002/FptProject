@model FPTBook.Models.Order
<h1>@TempData["alo"]</h1>
<div class="container">
    <table class="table table-bordered">
        <thead class="thead-dark">
            <tr>
                <th>No</th>
                <th>Email</th>
                <th>Date</th>
                <th>Title</th>
                <th>Quantity</th>
                <th>Total Price</th>
            </tr>
        </thead>
        <tbody>
            <tr>
                <td>1</td>
                <td>11</td>
                <td>@DateTime.Now.</td>
                <td>@Model.OrderName</td>
                <td>@Model.OrderQuantity</td>
                <td>@Model.OrderPrice</td>
            </tr>
        </tbody>
    </table>

</div> 
